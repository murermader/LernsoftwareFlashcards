<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="650.0" xmlns:fx="http://javafx.com/fxml/1"
            xmlns="http://javafx.com/javafx/2.2"
            fx:controller="ViewModel.DeckOverviewController">
    <VBox id="VBox" alignment="CENTER" spacing="10.0"
          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0"
          AnchorPane.topAnchor="0.0">
        <ListView fx:id="list" prefHeight="200.0" prefWidth="200.0"/>
        <HBox id="HBox" alignment="CENTER" spacing="5.0">
            <Button fx:id="deckAdd" mnemonicParsing="false" onAction="#handlerDeckAdd" text="Stapel hinzufügen"/>
            <Button fx:id="deckSelect" mnemonicParsing="false" onAction="#handlerDeckSelect" text="Stapel lernen"/>
            <Button fx:id="deleteDeck" mnemonicParsing="false" onAction="#handlerDeleteDeck" text="Stapel löschen"/>
            <Button fx:id="cardadd" mnemonicParsing="false" onAction="#handlerOverview" text="Kartenansicht"/>
            <Button fx:id="back" mnemonicParsing="false" onAction="#handlerBack" text="Zurück"/>
        </HBox>
    </VBox>
    <HBox alignment="CENTER_LEFT" fx:id="statusbar" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" prefHeight="25.0" prefWidth="650.0" AnchorPane.topAnchor="375.0">
        <Label fx:id="statusbarLabel1"/>
    </HBox>
</AnchorPane>

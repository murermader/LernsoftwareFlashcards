<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane maxHeight="400.0" maxWidth="650.0" minHeight="400.0" minWidth="650.0" prefHeight="400.0" prefWidth="650.0"
            xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ViewModel.UserAddController">
    <VBox id="VBox" alignment="CENTER" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <HBox id="HBox" alignment="CENTER" spacing="5.0">
            <Label text="Benutzername"/>
            <TextField fx:id="textField" prefWidth="200.0"/>
        </HBox>
        <HBox id="HBox" alignment="CENTER" spacing="5.0">
            <Button fx:id="confirmButton" mnemonicParsing="false" onAction="#handlerConfirm" text="Bestätigen"/>
            <Button fx:id="goBackButton" mnemonicParsing="false" onAction="#handlerBack" text="Zurück"/>
        </HBox>
    </VBox>
    <HBox alignment="CENTER_LEFT" fx:id="statusbar" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" prefHeight="25.0" prefWidth="650.0" AnchorPane.topAnchor="375.0">
        <Label fx:id="statusbarLabel1"/>
    </HBox>
</AnchorPane>

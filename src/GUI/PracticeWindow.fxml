<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PracticeWindowController">
  <children>
    <HBox id="HBox" alignment="CENTER" layoutY="336.0" spacing="5.0" AnchorPane.leftAnchor="10.0">
      <children>
        <Button fx:id="easy" disable="true" mnemonicParsing="false" onAction="#handlerEasy" prefHeight="20.0" prefWidth="115.0" text="Leicht" />
        <Button fx:id="ok" disable="true" mnemonicParsing="false" onAction="#handlerOk" prefHeight="20.0" prefWidth="115.0" text="Ok" />
        <Button fx:id="hard" disable="true" mnemonicParsing="false" onAction="#handlerHard" prefHeight="20.0" prefWidth="120.0" text="Schwer" />
        <Button fx:id="show" mnemonicParsing="false" onAction="#handlerShowBack" prefHeight="20.0" prefWidth="180.0" text="Rückseite anzeigen" />
        <Button fx:id="back" mnemonicParsing="false" onAction="#handlerBack" prefHeight="20.0" prefWidth="70.0" text="Zurück" />
      </children>
    </HBox>
    <VBox id="VBox" alignment="CENTER" layoutY="14.0" spacing="40.0" AnchorPane.leftAnchor="10.0">
      <children>
        <Label prefWidth="78.0" text="Frage" textAlignment="CENTER" />
        <Label fx:id="questionLabel" />
        <Label text="Antwort" textAlignment="CENTER" />
        <Label fx:id="answerLabel" />
      </children>
    </VBox>
    <Label fx:id="infoLabel" layoutY="14.0" prefHeight="35.0" prefWidth="259.0" textAlignment="JUSTIFY" textFill="#e40000" visible="false" AnchorPane.rightAnchor="30.0">
      <font>
        <Font size="12.0" />
      </font>
    </Label>
    <Label fx:id="easyTime" layoutX="10.0" layoutY="301.0" prefHeight="35.0" prefWidth="115.0" textAlignment="CENTER" />
    <Label fx:id="okTime" layoutX="133.0" layoutY="301.0" prefHeight="35.0" prefWidth="115.0" textAlignment="CENTER" />
    <Label fx:id="hardTime" layoutX="248.0" layoutY="301.0" prefHeight="35.0" prefWidth="120.0" textAlignment="CENTER" />
      <Label fx:id="currentUserLabel" layoutX="427.0" layoutY="67.0" text="yeet"/>
  </children>
</AnchorPane>
